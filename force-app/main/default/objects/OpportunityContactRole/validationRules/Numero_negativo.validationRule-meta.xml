<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Numero_negativo</fullName>
    <active>true</active>
    <description>evitar a inserção de números negativos</description>
    <errorConditionFormula>PorcentagemParticipacao__c &lt; 0</errorConditionFormula>
    <errorDisplayField>PorcentagemParticipacao__c</errorDisplayField>
    <errorMessage>Não é possível inserir valor negativo</errorMessage>
</ValidationRule>
